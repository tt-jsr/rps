#Invoke less on a filename list element
#[...]
# idx
<< idx STOL
l STOL
"less "
l RCLL
idx RCLL
GET
CAT
!
l RCLL
>>
interactive.less STO

#####################################################
#Invoke vim on a filename list element
#[...]
# idx
<< idx STOL
l STOL
"vim "
l RCLL
idx RCLL
GET
CAT
!
l RCLL
>>
interactive.vim STO

# Format list items into a numbers output
<< DUP
0 count STOL
FOR
    count RCLL
    "%0: %1" FORMAT PRINT
    1 ADD count STOL
    DROP
ENDFOR
>> interactive.show STO


