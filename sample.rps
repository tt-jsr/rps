<< 
"/" SPLIT
-1 GET
>> "basename" STO

>> "git branch --no-color | sed -e \"/^[^*]/d\" -e \"s/* \(.*\)/\1/\""
getbranch STO

<<
    "git rev-parse --is-inside-work-tree" PREAD
    IF "true" EQ
    THEN
        "git rev-parse --show-toplevel" PREAD
        0 GET
        basename()
    ENDIF
>> getrepo STO

